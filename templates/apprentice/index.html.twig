{% extends 'base.html.twig' %}
{% block title %}Dashboard!
{% endblock %}
{% block stylesheets %}
	{{ parent() }}
{% endblock %}
{% block body %}
	<div class="container">
		<div class="row justify-content-center">
			<div class-"col-xl-12">
				<h1 class="title">Dashboard
				</h1>
			</div>
		</div>
	</div>


	<div class="payed">

		<div class="container">

			<section class="application-progress-steps">


				<div class="row justify-content-center">
					<div class="col">
						<h3 class="design-process-title">Application Progress
						</h3>
					</div>
				</div>


				<div class="row">

					<div id="steps">
						<div class="step" data-desc="Basic Information">1</div>
						<div class="hide">
							<p>
								Questionnair pdf
							</p>
						</div>

						<div class="step " data-desc="Profile Setup">2</div>
						<div class="step active " data-desc="Placement Test">3</div>
						<div class="step " data-desc="Payment">4</div>

					</div>

				</div>


			</section>


		</div>


		<section>
			<div class="container-fluid ">

				<div
					class="row ml-5">

					{# start of course card #}
					<div class="col ">
						<div
							class="courses-section">
							{# card header #}
							<div class="card-header">

								<div class="container">
									<div class="row">
										<div class="col-6">
											<p>My courses
											</p>
										</div>
										<div class="col">
											<button class="btn-see-all">see all
											</button>
										</div>
									</div>

								</div>
								<div class="container">
									<div class="row">
										<div class="col d-flex justify-content-center">
											<img src="{{asset('image/mycourses.png')}}" class="college-pic">
										</div>
									</div>
								</div>

							</div>
							{# end card header #}

							{# start card content #}

							<div class="card-content">

								<div class="row">
									<div class="col">
										<p class="course-title">
											SAT math
										</p>
									</div>
								</div>

								<div class="row">
									<div class="col">
										<p class="course-lvl">
											Level 1
										</p>
									</div>
								</div>


								<div class="row">
									<div class="col">
										<div class="progress">
											<div class="progress-bar" role="progressbar" style="width: 25%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
										</div>
									</div>
								</div>


								<div class="row">
									<div class="col">
										<p class="courses-progress">
											Completed: 80%
										</p>
									</div>
								</div>


								<div class="row">
									<div class="col">
										<p class="title-next-session">
											Next Session
										</p>
									</div>
								</div>


								<div class="row">
									<div class="col">
										<p class="course-date">
											17/02/2020
										</p>
									</div>
								</div>


							</div>
							{# end card content #}
							<div class="row">
								<div class="col  d-flex justify-content-center">

									<div>
										<div class="dot active"></div>
										<div class="dot"></div>
										<div class="dot"></div>
										<div class="dot"></div>

									</div>

								</div>

							</div>
						</div>


					</div>
					{# end course card #}


					{# start of college card #}
					<div class="col mr-4">
						<div
							class="courses-section">
							{# card header #}
							<div class="card-header">

								<div class="container">
									<div class="row">
										<div class="col-6">
											<p>My college list
											</p>
										</div>
										<div class="col">
											<button class="btn-see-all">see all
											</button>
										</div>
									</div>

								</div>
								<div class="container">
									<div class="row">
										<div class="col d-flex justify-content-center">
											<img src="{{asset('image/college.png')}}" class="college-pic">
										</div>
									</div>
								</div>


							</div>
							{# end card header #}

							{# start card content #}
							<div class="card-content"></div>
							{# end card content #}

						</div>


					</div>
					{# end college card #}


					{# start of club card #}
					<div class="col ">
						<div
							class="courses-section">
							{# card header #}
							<div class="card-header">

								<div class="container">
									<div class="row">
										<div class="col-6">
											<p>My clubs
											</p>
										</div>
										<div class="col">
											<button class="btn-see-all">see all
											</button>
										</div>
									</div>

								</div>
								<div class="container">
									<div class="row">
										<div class="col d-flex justify-content-center">
											<img src="{{asset('image/club-pic.png')}}" class="college-pic">
										</div>
									</div>
								</div>

							</div>
							{# end card header #}

							{# start card content #}

							<div class="card-content"></div>
							{# end card content #}
						</div>


					</div>
					{# end club card #}


					{# start of documents card #}
					<div class="col ">
						<div
							class="courses-section">
							{# card header #}
							<div class="card-header">

								<div class="container">
									<div class="row">
										<div class="col-6">
											<p>Translated documents
											</p>
										</div>
										<div class="col">
											<button class="btn-see-all">see all
											</button>
										</div>
									</div>

								</div>
								<div class="container">
									<div class="row">
										<div class="col d-flex justify-content-center">
											<img src="{{asset('image/document-pic.png')}}" class="document-pic">
										</div>
									</div>
								</div>

							</div>
							{# end card header #}

							{# start card content #}

							<div class="card-content"></div>
							{# end card content #}

						</div>


					</div>
					{# end documents card #}


				</div>

			</div>

		</section>


	</div>


{% endblock %}
{% block javascripts %}{% endblock %}
